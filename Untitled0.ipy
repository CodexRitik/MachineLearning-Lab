import pandas as pd
import numpy as np
import matplotlib.pyplot as plt
def slope_intercept(x1,y1,x2,y2):
    a = (y2 - y1) / (x2 - x1)
    b = y1 - a * x1     
    return a,b
m,c = slope_intercept(3,5,9,10)
print(slope_intercept(3,5,9,10))
print("Equation of line : y = {0:.2f}.x + {1}".format(m,c))

